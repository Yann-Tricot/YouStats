        paginate:
        {
          "previous": "Previous page"
        },
        rowReorder: false,
        lengthMenu: [[10, 25, 50, -1], [10, 25, 50, "All"]],
        pageLength: 15,
        initComplete: function () {
          this.api().columns().every(function () {
            var that = this;
            
            $('input', this.footer()).on('keyup change clear', function () {
              if (that.search() !== this.value) {
                that
                .search(this.value)
                .draw();
              }
            });
          });
        },
        buttons: [
        { extend: 'csv', className: 'testBouton' },
        { extend: 'excel', className: 'testBouton' },
        { extend: 'pdfHtml5', className: 'testBouton', orientation: 'landscape', pageSize: 'A3' },
        { extend: 'print', className: 'testBouton' },
        ]
      })
    });      
    
    
    $('table tfoot th').each(function () {
      var title = $('table tfoot th').eq($(this).index()).text();
      $(this).html('<input type="text" placeholder="Search ' + title + '" />');
    });
    
    $.extend(true, $.fn.dataTable.defaults, {
      columnDefs: [
      {
        targets: '_all',
        defaultContent: '-'
      }
      ],
      language: {
        processing: '<i class="fa fa-spinner fa-spin fa-3x fa-fw"></i>Processing...'
      },
    });
    
    $('table tbody').on('dblclick', 'tr', function () {   
      
      
      var lien = cors + "https://i.ytimg.com/vi/dBX_mLT5f3c/sddefault.jpg";
      var lien2 = cors + "https://www.google.com/url?sa=i&url=https%3A%2F%2Fdeveloper.mozilla.org%2Ffr%2Fdocs%2FWeb%2FHTML%2FElement%2FImg&psig=AOvVaw18gr0YP4N_5daIKW_ssPag&ust=1606481499287000&source=images&cd=vfe&ved=0CAIQjRxqFwoTCKC8_JagoO0CFQAAAAAdAAAAABAD";
      var elt = document.getElementById("imageMiniature");
      elt.innerHTML = '';
      var name = "Nom : " + $('td', this).eq(0).text();
      var classe = "Classe : " + $('td', this).eq(1).text();
      var taille = "Taille : " + $('td', this).eq(2).text();
      var spécialité = "Spécialité : " + $('td', this).eq(3).text();
      $('#DescModal').modal("show");
      $('#nom').text(name);
      $('#classe').text(classe);
      $('#taille').text(taille);
      $('#spec').text(spécialité);
      var img = document.createElement("img");
      img.crossOrigin = "anonymous";
      img.src = lien;
      img.width = 200;
      img.height = 200;
      elt.appendChild(img);
    });
    
    
    
    
    
